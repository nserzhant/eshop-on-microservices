{
  "profiles": {
    "Docker Compose": {
      "commandName": "DockerCompose",
      "commandVersion": "1.0",
      "composeLaunchAction": "LaunchBrowser",
      "composeLaunchServiceName": "eshop.catalog.api",
      "composeLaunchUrl": "{Scheme}://localhost:{ServicePort}/swagger",
      "serviceActions": {
        "eshop.catalog.api": "StartDebugging",
        "sqlserver": "StartWithoutDebugging"
      }
    },
    "SQL Server": {
      "commandName": "DockerCompose",
      "commandVersion": "1.0",
      "serviceActions": {
        "eshop.catalog.api": "DoNotStart",
        "sqlserver": "StartWithoutDebugging"
      }
    }
  }
}